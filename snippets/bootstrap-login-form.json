{
  "login form with Bootstrap": {
    "prefix": "login",
    "body": [
      "form(action='${1:#}' method='${2:method}')",
      "\t.form-group",
      "\t\tlabel(for='${3:email}') ${4:Email}",
      "\t\tinput.form-control(type='${5:email}', name='${6:email}' placeholder='${7:Email}' autofocus=true)",
      "\t.form-group",
      "\t\tlabel(for='${8:password}') ${9:Password}",
      "\t\tinput.form-control(type='${10:password}', name='{$11:password}')",
      "\tbutton.btn.btn-${12:primary}(type='submit') ${13:Log-in}"
    ],
    "description": "bootstrap login form"
  }
}