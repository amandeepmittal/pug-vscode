{
  "extends": {
    "prefix": "ext",
    "body": "extends $1"
  },
  "include": {
    "prefix": "inc",
    "body": "include ./includes/$1"
  },
  "blockname": {
    "prefix": "block",
    "body": "block $1"
  },
  "render file": {
    "prefix": "pugrf",
    "body": "pug.renderFile($1);"
  },
  "mixin": {
    "prefix": "mixin",
    "body": "mixin $1"
  }
}